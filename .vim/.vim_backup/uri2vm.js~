            <script>
//作者 li.sun@qunar.com
//wiki支持思维导图
function mindRender(){
    jQuery.getScript("http://s.qunarzz.com/wiki/kityminder.core.min.js",function(data, textStatus, jqxhr ){
        jQuery("pre.minder").each(function(){
        
                //组装脑图数据
                var content=jQuery('<pre id="minder-view'+containerId+'" minder-data-type="markdown" style="overflow:hidden">'+jQuery(this).text()+'</pre>');
                jQuery(this).html(content);
                var km = window.km = new kityminder.Minder();
                var renderTimes=0;
                km.on("layoutfinish",function(){
                    if(renderTimes<2){
                        var mindHeight=content.find("g").get(0).getBBox().height;
                        console.log(renderTimes);
                        content.height(mindHeight);


                        km.fire("paperrender");
                    }
                    renderTimes++;
                });
                km.setup('#minder-view'+containerId);
                km.disable();
                km.execCommand('hand');
                containerId++;
          
        });

    });

}


if(jQuery(".wiki-content").length>0 &&jQuery.browser.msie==false){
    
    var containerId=1;
    jQuery(document).ready(function(){
        jQuery('head').append( jQuery('<link rel="stylesheet" type="text/css" />').attr('href', 'http://s.qunarzz.com/wiki/kityminder.css') );
        jQuery.getScript("http://s.qunarzz.com/wiki/kity.min.js",function(data, textStatus, jqxhr){
            mindRender();
        });

    });
}

</script>
