<script type="text/javascript">
    NTES.ready(function() {
        var areas = $('.js-xwtj'),
                contents = $('.js-xwtj-content'),
                countArr = [0, 1, 2].sort(function(){ return 0.5 - Math.random(); }),
                content_cnxh = $('#js-cnxh-content');
        if(window.epTools && window.epTools.getRadomArray) {
            countArr = window.epTools.getRadomArray([0, 1, 2], 3);
        }
        var api_xwtjpr = "http://j.news.163.com/hy/demorecdocapi.s?limit=16&callback=xwtjpr";
        window.xwtjpr = function(data){
            var xwtjData = data,
                    html = [],
                    len_max = 16,
                    num,
                    re = /\'|\"/gi,
                    alttitle;
            html[0] = [];
            html[1] = [];
            html[2] = [];
            var tjtype = { '0': '#jnewsartrside1', '1': '#jnewsartrside2','2': '#jnewsartrside3'};
            if(xwtjData.length >= len_max) {
                for(var i = 0; i < len_max; i++){
                    var j = 0;
                    if(i > 7){
                        j = 1;
                    }
                    if(i > 11){
                        j = 2;
                    }
                    if(i == 0 || i == 8 || i == 12){
                        html[j].push('<h2 class="ep-title-3"><a href="' + xwtjData[i].link + tjtype[0] + '" title="' + xwtjData[i].title + '">' + xwtjData[i].title + '</a></h2><ul class="mod-f12list">');
                    }else{
                        html[j].push('<li><a href="' + xwtjData[i].link +  tjtype[0] + '" title="' + xwtjData[i].title + '">' + xwtjData[i].title + '</a></li>');
                    }
                    if(i == 7 || i == 11 || i == 15){
                        html[j].push('</ul>');
                    }
                };
                areas[0].innerHTML = html[0].join('');
                areas[1].innerHTML = html[1].join('');
                if(content_cnxh) {
                    content_cnxh.innerHTML = html[2].join('');
                }
            } else {
                for(var i = 0; i < areas.length; i++) {
                    areas[i].innerHTML = contents[countArr[i]].innerHTML;
                }
            }
        };
        NTES.ajax.importJs(api_xwtjpr, function(){}, 'utf-8')
    });
</script>
