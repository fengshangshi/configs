<script type="text/javascript">
//二维码
(function() {
    
// IE6 动态生成二维码速度太慢 因此去掉
if(!-[1,]&&!window.XMLHttpRequest) {
    return false;
}
NTES.ready(function() {   
    
    var wxicon2dIcon = NTES('.nt-wx2dcode-icon');
    var wxicon2dPop = NTES('.nt-wx2dcode-pop');
    var wxicon2dPic = NTES('.nt-wx2dcode-pic');
    var wxicon2dClose = NTES('.nt-wx2dcode-close');
    
    var icon2dIcon = NTES('.nt-2dcode-icon')
    var icon2dPop = NTES('.nt-2dcode-pop');
    var icon2dPic = NTES('.nt-2dcode-pic');
    var wxicon2dIcon3 = NTES('.nt-wx2dcode-icon3');
    var wxqrcode, qrcode, timer;   
    var nxkey, ntit, nurl, tjurl, img;
    setTimeout(function() {
        wxqrcode = new QRCode(wxicon2dPic[0], {
            width : 120,
            height : 120
        });
        wxqrcode.makeCode(window.location.href + "#sns_weixin");
    }, 500);
    
    setTimeout(function() {
        qrcode = new QRCode(icon2dPic[0], {
            width : 110,
            height : 110
        });
               
        qrcode.makeCode(window.location.href + "#2code");
    }, 1000);
    
    wxicon2dIcon.addEvent('click', function() {
        var offset = getOffset(this);
        wxicon2dPop[0].style.display = 'block';
        wxicon2dPop[0].style.left = offset.left - 86 + 'px';
        wxicon2dPop[0].style.top = offset.top + this.clientHeight + 8 + 'px';
        nxkey = Math.random().toString().substr(2,6);
        ntit = escape(document.title);
        nurl = escape(window.location.protocol.replace(":","") + '://snstj.' + window.location.host + window.location.pathname+'?snstj_weixin');
        tjurl='http://analytics.163.com/ntes?_nacc=snstj&_nvid=VISITOR_CLIENT_NO_COOKIE_SUPPORT&_nvtm=0&_nvsf=0&_nvfi=0&_nlag=&_nlmf=0&_nres=&_nscd=&_nstm=0&_nurl='+nurl+'&_ntit='+ntit+'&_nref=&_nfla=&_nssn=&_nxkey='+nxkey+'&_end1';
        img = new Image();
        img.src = tjurl;
    });
    wxicon2dIcon3.addEvent('click', function() {
        var offset = getOffset(this);
        wxicon2dPop[0].style.display = 'block';
        wxicon2dPop[0].style.left = offset.left - 86 + 'px';
        wxicon2dPop[0].style.top = offset.top + this.clientHeight + 8 + 'px';
        nxkey = Math.random().toString().substr(2,6);
        ntit = escape(document.title);
        nurl = escape(window.location.protocol.replace(":","") + '://snstj.' + window.location.host + window.location.pathname+'?snstj_weixin');
        tjurl='http://analytics.163.com/ntes?_nacc=snstj&_nvid=VISITOR_CLIENT_NO_COOKIE_SUPPORT&_nvtm=0&_nvsf=0&_nvfi=0&_nlag=&_nlmf=0&_nres=&_nscd=&_nstm=0&_nurl='+nurl+'&_ntit='+ntit+'&_nref=&_nfla=&_nssn=&_nxkey='+nxkey+'&_end1';
        img = new Image();
        img.src = tjurl;
    });
    wxicon2dClose.addEvent('click', function() {
        wxicon2dPop[0].style.display = "none";
    });  
    icon2dIcon.addEvent('mouseover', function() {
        var offset = getOffset(this);
        icon2dPop[0].style.display = 'block';
        icon2dPop[0].style.left = offset.left + 'px';
        icon2dPop[0].style.top = offset.top + this.clientHeight + 8 + 'px';
        clearTimeout(timer);
    });
    icon2dIcon.addEvent('mouseout', function() {
        timer = setTimeout(function() {
            icon2dPop[0].style.display = 'none';
        }, 100);
    });
    icon2dPop.addEvent('mouseover', function() {
        clearTimeout(timer);
    });
    
    icon2dPop.addEvent('mouseout', function(e) {
        
        var event = e || window.event;
        var relatedTarget = event.relatedTarget || event.toElement;
        
        if(!isParent(relatedTarget, icon2dPop[0])) {
            icon2dPop[0].style.display = 'none';
        }
        
    });
});
    
function getOffset(el) {
    var top = el.offsetTop;
    var left = el.offsetLeft;
    
    while(el.offsetParent){
        el = el.offsetParent;
        top += el.offsetTop;
        left += el.offsetLeft;
    }
    
    return {
        left: left,
        top: top
    }
}
    
function isParent(el, parent) {
    while (el && el.tagName != 'BODY'){
        if (el == parent) {
            return true;
        }
        el = el.parentNode;
    }
    
    return false;
    
}
})();    
</script>
